set output "sort4.ps"
set terminal postscript color "landscape"
set title "Temps nécéssaire"

plot \
"sort4.dat" using 1:2 title "tri par selection" with lines linetype 1, \
"sort4.dat" using 1:3 title "tri par insertion" with lines linetype 2, \
"sort4.dat" using 1:4 title "tri par quicksort" with lines linetype 3